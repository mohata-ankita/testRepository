{
  "name": "ExcelTable1",
  "type": "ExcelTable",
  "size": "Full row",
  "titleEnable": true,
  "title": "Test configuration",
  "plugin": "hxatb",
  "static": false,
  "query": {
    "name": "excelTableQuery",
    "query_params": [
      {
        "select": [
          "Cluster Name1=clusterName.term",
          "Model1=modelNumber.term",
          "Hypervisor1=esxiVersion.term",
          "CVM Linux Ver1=cvmLinuxVer.term",
          "Hx Version1=productVersion.term",
          "Result Path1=resultPath.term",
          "#Active Nodes1=activeNodes",
          "Dedup1=dedup",
          "Compression1=compression",
          "RF1=dataReplicationFactor",
          "Encryption1=diskEncryption",
          "Tag1=_tagBaseline_Tag.term",
          "Notes1=_tagNotes.term",
          "Desc1=_tagDescription.term"
        ],
        "where": "_tag_appName == $value AND _plugin == hxatb AND _documentType == testRunDetails AND _tag_runId == DropDown10.$value"
      },
      {
        "select": [
          "Cluster Name2=clusterName.term",
          "Model2=modelNumber.term",
          "Hypervisor2=esxiVersion.term",
          "CVM Linux Ver2=cvmLinuxVer.term",
          "Hx Version2=productVersion.term",
          "Result Path2=resultPath.term",
          "#Active Nodes2=activeNodes",
          "Dedup2=dedup",
          "Compression2=compression",
          "RF2=dataReplicationFactor",
          "Encryption2=diskEncryption",
          "Tag2=_tagBaseline_Tag.term",
          "Notes2=_tagNotes.term",
          "Desc2=_tagDescription.term"
        ],
        "where": "_tag_appName == $value AND _plugin == hxatb AND _documentType == testRunDetails AND _tag_runId == DropDown27.$value"
      }
    ],
    "from": "testbed_read",
    "time": "Last 1 month"
  },
  "tableRows": {
    "Cluster Name": [
      "Cluster Name1",
      "Cluster Name2"
    ],
    "Model": [
      "Model1",
      "Model2"
    ],
    "Hypervisor": [
      "Hypervisor1",
      "Hypervisor2"
    ],
    "CVM Linux Ver": [
      "CVM Linux Ver1",
      "CVM Linux Ver2"
    ],
    "Hx Version": [
      "Hx Version1",
      "Hx Version2"
    ],
    "Result Path": [
      "Result Path1",
      "Result Path2"
    ],
    "#Active Nodes": [
      "#Active Nodes1",
      "#Active Nodes2"
    ],
    "Dedupe": [
      "Dedup1",
      "Dedup2"
    ],
    "Compression": [
      "Compression1",
      "Compression2"
    ],
    "RF": [
      "RF1",
      "RF2"
    ],
    "Encryption": [
      "Encryption1",
      "Encryption2"
    ],
    "Baseline Tag": [
      "Tag1",
      "Tag1"
    ],
    "Notes": [
      "Notes1",
      "Notes2"
    ],
    "Description": [
      "Desc1",
      "Desc2"
    ]
  },
  "tableHeaders": [
    "Run 1",
    "Run 2"
  ],
  "height": "3"
}