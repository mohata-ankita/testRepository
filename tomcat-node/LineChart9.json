{
  "name": "LineChart9",
  "type": "LineChart",
  "size": "Half row",
  "title": "Comp Class Utilization",
  "plugin": "tomcat",
  "static": false,
  "stacked": false,
  "dataPoints": "Off",
  "sideLegend": false,
  "adaptiveInterval": true,
  "dropdowns": [
    {
      "name": "d1",
      "type": "metrics",
      "query": "lineChartMetric"
    }
  ],
  "query": {
    "name": "lineChartMetric",
    "select": [
      "bfGcCompClass = bfGcCompClassUsed *100/bfGcCompClassCommitted",
      "afcCompClass = afGcCompClassUsed*100/afGcCompClassCommitted"
    ],
    "from": "snappyflow_read",
    "where": "_tag_appName == $value AND _tag_Name == $value AND _plugin == tomcat",
    "time": "Last 1 month",
    "histogram": "30m"
  },
  "metricsFilter": {}
}